__Letter "#include <std.h>#include <move.h>#include <illuminati.h>inherit ARMOUR;void create() {   ::create();     set_property(\"no steal\", 1);     set_name(\"Illuminati band\");     set_short(\"Member's Band\");     set_long(\"If you are a part of the Illuminati you may wear this as a show of rank. You can only wear one at a time.\");     set_id(({ \"metal band\", \"bronze band\", \"silver band\", \"gold band\", \"band\"}));     set_ac(1);     set_weight(2);     set_curr_value(\"gold\", 150);     set_type(\"bracelet\");     set_limbs(({\"left hand\", \"right hand\"}));}void init() {   ::init();     add_action(\"readme\", \"read\");}int query_auto_load() {    if(this_player()->query_guild() == \"illuminati\") return 1;    return 0;}int readme(string str) {    object board;    board = present(\"__ilboard\", load_object(ILROOM\"board.c\"));    if(!board || !board->is_board())      return notify_fail(\"!ERROR! Contact Amarael [ Could not find board ]\\n\");    return (board->read_message(str));}    int move(mixed dest) {    if(objectp(dest)) {      if(dest->is_player()) {        if(dest->query_guild() == \"illuminati\") {          changeme(dest->query_guild_position());          message(\"info\", (ILOBJ\"postalert\")->query_unread(dest->query_name()), dest, );          }        else          return MOVE_NOT_ALLOWED;        }      }    return ::move(dest);}void changeme(string posi) {     switch(posi) {        case \"squire\" :			set_name(\"metal band\");			set_short(\"%^RESET%^Metal band\");			set_long(\"This metal band signifies your rank as a Mortal in the Illuminati.\");			set_value(0);                        break;	case \"member\" :			set_name(\"bronze band\");			set_short(\"%^ORANGE%^Br%^RESET%^on%^ORANGE%^ze B%^RESET%^an%^ORANGE%^d%^RESET%^\");			set_long(\"This bronze band signifies your rank as a Hero in the Illuminati.\");			set_value(0);			break;	case \"council\" :			set_name(\"silver band\");			set_short(\"%^BOLD%^WHITE%^S%^RESET%^i%^BOLD%^l%^RESET%^v%^BOLD%^e%^RESET%^r %^BOLD%^b%^RESET%^a%^BOLD%^n%^RESET%^d\");			set_long(\"This silver band signifies your rank as a Legend in the Illuminati.\");			set_value(0);			break;	case \"leader\" :			set_name(\"gold band\");			set_short(\"%^ORANGE%^G%^YELLOW%^ol%^WHITE%^d b%^YELLOW%^an%^RESET%^ORANGE%^d%^RESET%^\");			set_long(\"This gold band signifies your rank as an Immortal in the Illuminati.\");			set_value(0);			break;	}}int give() { write(\"You may not give this away.\"); return 1; }int drop() { write(\"Whatever for?\"); return 1; }"
__Undeleted ({"ug",})
