__Letter "// Display triangles side by side			for (lineCount = 1; lineCount <= 10; lineCount++)			{				// Pattern A				for(count = 1; count <= lineCount; count++)				{					Console.Write(\"*\");						}				for (spacesCount = 10; spacesCount >= lineCount; spacesCount--)				{					Console.Write(\">\");				}				// Pattern B				for (count = 10; count >= lineCount; count--)				{					Console.Write(\"*\");				}				for (spacesCount = 1; spacesCount <= lineCount; spacesCount+= 2)				{					Console.Write(\">\");				}				// Pattern C				for (count = 10; count >= lineCount; count--)				{					Console.Write(\"*\");				}				for (count = 10; count >= lineCount; count--)				{					Console.Write(\">\");				}				// Pattern D				for (count = 1; count <= lineCount; count++)				{					Console.Write(\"*\");				}				Console.WriteLine();			}"
