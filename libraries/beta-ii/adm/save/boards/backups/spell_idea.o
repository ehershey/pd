1;   }   if(this_player()->query_disable()) return 1;   this_player()->set_disable();   dmg = this_player()->query_skill(\"attack\") -        ob->query_skill(\"defense\") +        this_player()->query_stats(\"strength\")*6 +        this_player()->query_stats(\"dexterity\")*5 -        ob->query_stats(\"dexterity\")*1/3 -        ob->query_stats(\"constitution\") +        this_player()->query_skill(\"melee\")/2;   if(dmg < ob->query_hp()) {       write(\"You try to execute a finishing blow, but fail!\\nYou are thrown terribly off balance from your misjudgement!\");       say(this_player()->query_cap_name()+\" tries to execute a Coup De Grace on \"+ob->query_cap_name()+\" and fails, making \"+objective(this_player())+\" loose \"+possessive(this_player())+\" balance.\", ob);       message(\"info\",this_player()->query_cap_name()+\" tries to execute a Coup De Grace on you, but falls off balance!\", ob);       ob->damage(dmg/10);       this_player()->set_paralyzed(2+random(2), \"You have yet to regain your balance after your attempt to finish off \"+ob->query_cap_name()+\"!\");   }   else {       write(\"%^BOLD%^%^RED%^In a spray of blood, you finish off \"+ob->query_cap_name()+\".%^RESET%^\");       say(\"%^BOLD%^%^RED%^\"+this_player()->query_cap_name()+\" finishes off \"+ob->query_cap_name()+\" in a spray of blood.%^RESET%^\", ob);       message(\"info\", \"%^BOLD%^%^RED%^\"+this_player()->query_cap_name()+\" cleaves through you, mortally wounding you.%^RESET%^\", ob);       ob->damage(dmg*2);   }   this_player()->add_sp(dmg/5);   return 1;}int abil() {   object o;   o = this_player();   if (this_player()->query_class() != \"figher\" ||       this_player()->query_subclass() != \"swashbuckler\" ||       this_player()->query_subclass() == \"paladin\") return 0;   if(o->query_level() < 40) return 0;   return 1;}void help() {   write(\"Syntax: <coupdegrace>\\n\\n\"      \"Powerful fighters are able to execute a Coup De Grace, delivering a finishing blow to their enemy.  If miscalculated, it throws the fighter off balance.\\n\"); }","owner":"nightshade",]),(["size":83,"date":1032137246,"title":"PS","message":"I've got a copy in /wizards/nightshade/_coupdegrace.c if you want to use that one.","owner":"nightshade",]),(["size":183,"date":1032326113,"title":"on spell_idea","message":"Something to take a look at:  BardsIt has come to my attention that bards might be able to sing while there a ghost,  but not talk.  I dont know if this is true or not thoughWhit","owner":"whit",]),(["size":220,"date":1032327323,"title":"Problem with slash","message":"Theres a big problem with slash.  You can slash objects that arent monsters or players.  There is no check to see if the person is living.  So in plain words and in more logic,  you could kill a ghost with slash.Whit","owner":"whit",]),(["size":279,"date":1032372993,"title":"blah","message":"Elven village is all screwed up. I am reworking the entire map, I am going to keep the same mobs and items. Thinking of using several virtual areas as an experiment, like 4-6 servers and mixing the virtual descripts with static descripts for important rooms. Hope this goes well","owner":"stormbringer",]),(["size":157,"date":1032390826,"title":"Elven village, raknids","message":"I can redo descrips for the elven village, but it would be room by room walking through it. I can also do an underground raknid thing for ya if you need it.","owner":"wraith",]),(["size":242,"date":1032482489,"title":"follower code","message":"pl = this_object()->query_current_attacker();if(environment(this_object()) != environment(pl)) {      this_object()->move(environment(pl));      message(\"info\", \"A monster enters intent on killing you.\\n\", environment(pl));   return 1;}","owner":"stormbringer",]),(["size":169,"date":1032483590,"title":"prvent wear code","message":"    set_wear((: extra_worn :));int extra_worn() {    if((int)this_player()->query_guild() != \"forsake