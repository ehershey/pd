NEW FUNCTIONS:
void   set_devobtimestart(int x)    { set("timestart", x); }
void   set_devobtimeperiod(int x)   { set("timeperiod", x); }
void   set_dev_value(int x)      { set("dev_value", x); }
void   set_dev_exp(int x)        { set("dev_exp", x); }
int    query_devtimestart()      { return
(int)this_object()->query("timestart");}
int    query_dev_value()         { return
(int)this_object()->query("dev_value");}
int    query_dev_exp()           { return
(int)this_object()->query("dev_exp");}
int    query_devtimeperiod()     { return
(int)this_object()->query("timeperiod");}
